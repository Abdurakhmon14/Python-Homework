import sqlite3

create_table = """
Create table People 
(FirstName TEXT, LastName TEXT, Age INT);
"""
insert_table = """
Insert into People values
('Bob', 'Johnson', 35),
('Alex', 'Robertson', 24);
"""

con = sqlite3.connect('test.db')
cursor = con.cursor()
cursor.execute(create_table)
cursor.execute(insert_table)
con.commit()
con.close()
